{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "colorbutton",
                "version": "4.6",
                "type": "drupal-library",
                "dist": {
                    "url": "http://download.ckeditor.com/colorbutton/releases/colorbutton_4.6.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "font",
                "version": "4.6",
                "type": "drupal-library",
                "dist": {
                    "url": "http://download.ckeditor.com/font/releases/font_4.6.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "panelbutton",
                "version": "4.6",
                "type": "drupal-library",
                "dist": {
                    "url": "http://download.ckeditor.com/panelbutton/releases/panelbutton_4.6.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "owlcarousel",
                "version": "2.2.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/OwlCarousel2/OwlCarousel2/archive/2.2.1.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "1.3.0",
        "drupal-composer/drupal-scaffold": "2.3.0",
        "cweagans/composer-patches": "1.6",
        "drupal/core": "8.3.7",
        "drush/drush": "~8.3",
        "drupal/console": "~1.0",
        "drupal/features": "3.5",
        "drupal/entity": "1.0-alpha4",
        "drupal/media_entity": "1.6",
        "drupal/media_entity_image": "1.2",
        "drupal/media_entity_browser": "1.0-beta3",
        "drupal/entity_browser": "1.1",
        "drupal/inline_entity_form": "1.0-beta1",
        "drupal/ctools": "3.0",
        "drupal/token": "1.0",
        "drupal/video_embed_field": "1.5",
        "drupal/entity_reference_revisions": "1.3",
        "drupal/media_entity_embeddable_video": "1.0-beta2",
        "drupal/diff": "1.0-rc1",
        "drupal/paragraphs": "1.1",
        "drupal/devel": "1.0",
        "drupal/pathauto": "1.0",
        "drupal/metatag": "1.2",
        "drupal/robotstxt": "1.1",
        "drupal/simple_sitemap": "2.9",
        "drupal/ckeditor_font": "1.0",
        "drupal/colorbutton": "1.1",
        "drupal/shield": "1.1",
        "drupal/partner_link": "1.0-alpha2",
        "drupal/photos": "4.0-rc2",
        "colorbutton": "4.6",
        "font": "4.6",
        "panelbutton": "4.6",
        "owlcarousel": "2.2.1"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "config": {
        "secure-http": false
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
